{
  "manifest_version": 2,

  "name": "InstaBS",
  "description": "This extension allows you to open a sessions at Browserstack instantly",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*"
  ]
}
